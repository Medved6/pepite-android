<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_after_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.sergewsevolojsky.pepiteapp.activity.AfterRegisterActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <fragment android:name="com.example.sergewsevolojsky.pepiteapp.fragments.NotificationPermissionFragment"
            android:id="@+id/list_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout="@layout/fragment_notification_permission" />

        <fragment android:name="com.example.sergewsevolojsky.pepiteapp.fragments.GoToSportFragment"
            android:id="@+id/list_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout="@layout/fragment_go_to_sport" />

    </LinearLayout>



</RelativeLayout>
